    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "4.0.0-beta.9",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
