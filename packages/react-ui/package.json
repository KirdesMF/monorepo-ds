    "@kirdes-org/utils": "workspace:*",
    "@storybook/react": "^8.5.0",
    "@tailwindcss/vite": "4.0.0-beta.9",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
